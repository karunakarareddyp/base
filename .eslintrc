{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "babel",
    "react"
  ],
  "rules": {
    "no-unused-vars": 0,
    "no-console": "off",
    "class-methods-use-this": 0,
    "max-len": [1, 160, 2, {"ignoreComments": true}],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/prop-types": ["error", {
      "ignore": ["className", "style"]
    }
    ],
    "react/jsx-filename-extension": "off",
    "indent": ["error", 4]
  },
  "extends": ["airbnb-base"]
}